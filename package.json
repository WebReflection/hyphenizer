{
  "name": "hyphenizer",
  "version": "0.1.0",
  "description": "Transforms PascalCase to hyphen-case",
  "main": "cjs/index.js",
  "module": "esm/index.js",
  "unpkg": "index.js",
  "scripts": {
    "build": "npm run cjs && npm run esm",
    "cjs": "cp index.js cjs && echo 'module.exports = hyphenizer;'>>cjs/index.js",
    "esm": "cp index.js esm && echo 'export default hyphenizer;'>>esm/index.js"
  },
  "keywords": [
    "hyphen",
    "pascal",
    "case",
    "converter"
  ],
  "author": "Andrea Giammarchi",
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/WebReflection/hyphenizer.git"
  },
  "bugs": {
    "url": "https://github.com/WebReflection/hyphenizer/issues"
  },
  "homepage": "https://github.com/WebReflection/hyphenizer#readme"
}
